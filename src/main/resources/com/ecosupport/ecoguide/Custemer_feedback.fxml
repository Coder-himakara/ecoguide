<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="680.0" prefWidth="980.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller=" com.ecosupport.ecoguide.Custemer_feedbackController">
    <children>
        <ImageView fitHeight="680.0" fitWidth="980.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@feedback_image/Custemer_feedback.png" />
            </image>
        </ImageView>
        <Text layoutX="237.0" layoutY="318.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
        <TextField id="feedback_name" layoutX="445.0" layoutY="295.0" prefHeight="33.0" prefWidth="256.0" promptText="Jhone Dave" />
        <Text layoutX="236.0" layoutY="365.0" strokeType="OUTSIDE" strokeWidth="0.0" text="E-mail">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
        <TextField id="feedback_mail" layoutX="445.0" layoutY="340.0" prefHeight="33.0" prefWidth="256.0" promptText="example@gmail,com" />
        <Text layoutX="235.0" layoutY="417.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Feedback">
            <font>
                <Font size="20.0" />
            </font>
        </Text>
        <TextArea id="feedback" layoutX="440.0" layoutY="399.0" prefHeight="140.0" prefWidth="265.0" promptText="Enter your feedback" />
        <Button id="home" layoutX="837.0" layoutY="20.0" mnemonicParsing="false" onAction="#goToHome" style="-fx-border-color: green;" text="Home">
            <font>
                <Font size="18.0" />
            </font>
            <cursor>
                <Cursor fx:constant="MOVE" />
            </cursor>
        </Button>
        <Button id="animal" layoutX="726.0" layoutY="20.0" mnemonicParsing="false" onAction="#goToAnimal" style="-fx-border-color: green;" text="Animal">
            <font>
                <Font size="18.0" />
            </font>
            <cursor>
                <Cursor fx:constant="MOVE" />
            </cursor>
        </Button>
        <Button id="plant" layoutX="625.0" layoutY="20.0" mnemonicParsing="false" onAction="#goToPlant" style="-fx-border-color: green;" text="Plant">
            <font>
                <Font size="18.0" />
            </font>
            <cursor>
                <Cursor fx:constant="MOVE" />
            </cursor>
        </Button>
        <Button id="send" layoutX="236.0" layoutY="489.0" mnemonicParsing="false" onAction="#saveData" style="-fx-background-color: green;" text="Send" textFill="WHITE">
            <font>
                <Font size="20.0" />
            </font>
            <cursor>
                <Cursor fx:constant="MOVE" />
            </cursor>
        </Button>
    </children>
</AnchorPane>
